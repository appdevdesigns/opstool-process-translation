<div class="form-inline">
    <div class="form-group">
        <label for="translateFromLanguage">From:</label>
        <select id="translateFromLanguage" class="language-selector" trrequest-trans="fromLanguage">
            <% for (var code in fromLanguagesList.attr()) { %>
                <option value="<%= code %>" <%=code===fromLanguageCode ? "selected" : "" %>>
                    <%= fromLanguagesList[code] %>
                </option>
                <% } %>
        </select>
    </div>
    <div class="form-group">
        <label for="translateToLanguage">To:</label>
        <select id="translateToLanguage" class="language-selector" trrequest-trans="toLanguage">
            <% for (var code in toLanguagesList.attr()) { %>
                <option value="<%= code %>" <%=code===toLanguageCode ? "selected" : "" %>>
                    <%= toLanguagesList[code] %>
                </option>
                <% } %>
        </select>
    </div>
</div>